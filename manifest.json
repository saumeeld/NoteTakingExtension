{
  "manifest_version": 2,
  "name": "BrowserNotes",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": ["jquery.js", "jquery-ui.js", "content.js"],
      "css": ["notepad.css"]
    }
  ],
  "browser_action": {
  	"default_icon": "notes.png"
	},
  "background": {
  	"scripts": []
	},
	"permissions": [
        "storage"
    ]
}

